{
  "name": "x0xmd",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "test": "node --test test/*.test.js",
    "check:release-evidence": "node --test test/release-evidence-schema.test.js test/release-operations.test.js",
    "check:drift": "node --test test/drift-contract.test.js && node scripts/check-propagation-drift.mjs",
    "refresh:upstream": "node scripts/ops/refresh-upstream.mjs",
    "check:interop": "node --test test/interop-conformance.test.js",
    "ci": "npm run test && npm run check:release-evidence && npm run check:drift && npm run check:interop && npm run deploy -- --dry-run",
    "security:audit": "npm audit --audit-level=low",
    "dev": "npm exec wrangler -- dev",
    "deploy": "npm exec wrangler -- deploy"
  },
  "devDependencies": {
    "wrangler": "^4.69.0"
  }
}
